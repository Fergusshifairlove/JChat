<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.unitec.java.chat.client.RegisterController">
   <children>
      <Pane prefHeight="200.0" prefWidth="300.0">
         <children>
            <Label fx:id="lblNickName" layoutX="62.0" layoutY="63.0" prefHeight="16.0" prefWidth="139.0" text="Enter a nick name" />
            <Button fx:id="btnJoinChat" layoutX="150.0" layoutY="130.0" mnemonicParsing="false" onAction="#joinChat" text="Join Chat" />
            <TextField fx:id="txtNickName" layoutX="62.0" layoutY="87.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
